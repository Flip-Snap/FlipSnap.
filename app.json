{
  "expo": {
    "name": "FlipSnap",
    "slug": "FlipSnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.flipsnap.FlipSnap"
    },
    "web": {
      "favicon": "./images/icon.png"
    },
    "plugins": [
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow Voice to Text Tutorial to access the microphone",
          "speechRecognitionPermission": "Allow Voice to Text Tutorial to securely recognize user speech"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7261c31b-e88a-4441-ba4e-cbc432950347"
      }
    }
  }
}
